ackage com.company;
import java.util.Scanner;
public class Main {

    public static void main(String[] args) {
	      Scanner year = new Scanner(System.in);
        System.out.println("Hello! Tell me the year of your birth, please");
        int yb = year.nextInt();
        System.out.println("Tell me what year it is at this moment, please");
        int yn = year.nextInt();
        int old = yn - yb;
        System.out.println("You are "+ old +" years old");
    }
}
